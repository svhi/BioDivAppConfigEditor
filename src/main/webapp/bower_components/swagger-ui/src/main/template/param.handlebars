<td class='code'>{{qName}}</td>
<td>

	{{#if isBody}}
		{{#if isFile}}
			<input type="file" qName='{{qName}}'/>
			<div class="parameter-content-type" />
		{{else}}
			{{#if defaultValue}}
				<textarea class='body-textarea' qName='{{qName}}'>{{defaultValue}}</textarea>
			{{else}}
				<textarea class='body-textarea' qName='{{qName}}'></textarea>
				<br />
				<div class="parameter-content-type" />
			{{/if}}
		{{/if}}
	{{else}}
		{{#if isFile}}
			<input type="file" qName='{{qName}}'/>
			<div class="parameter-content-type" />
		{{else}}
			{{#if defaultValue}}
				<input class='parameter' minlength='0' qName='{{qName}}' placeholder='' type='text' value='{{defaultValue}}'/>
			{{else}}
				<input class='parameter' minlength='0' qName='{{qName}}' placeholder='' type='text' value=''/>
			{{/if}}
		{{/if}}
	{{/if}}

</td>
<td>{{{description}}}</td>
<td>{{{paramType}}}</td>
<td>
	<span class="model-signature"></span>
</td>
