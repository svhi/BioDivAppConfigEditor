<td class='code required'>{{qName}}</td>
<td>
	{{#if isBody}}
		{{#if isFile}}
			<input type="file" qName='{{qName}}'/>
		{{else}}
			{{#if defaultValue}}
				<textarea class='body-textarea' placeholder='(required)' qName='{{qName}}'>{{defaultValue}}</textarea>
			{{else}}
				<textarea class='body-textarea' placeholder='(required)' qName='{{qName}}'></textarea>
				<br />
				<div class="parameter-content-type" />
			{{/if}}
		{{/if}}
	{{else}}
		{{#if isFile}}
			<input class='parameter' class='required' type='file' qName='{{qName}}'/>
		{{else}}
			{{#if defaultValue}}
				<input class='parameter required' minlength='1' qName='{{qName}}' placeholder='(required)' type='text' value='{{defaultValue}}'/>
			{{else}}
				<input class='parameter required' minlength='1' qName='{{qName}}' placeholder='(required)' type='text' value=''/>
			{{/if}}
		{{/if}}
	{{/if}}
</td>
<td>
	<strong>{{{description}}}</strong>
</td>
<td>{{{paramType}}}</td>
<td><span class="model-signature"></span></td>
